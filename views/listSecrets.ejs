<table class="table table-striped table-hover">
    <thead>
    <tr>
        <th>Secret Name</th>
        <th>URL</th>
        <th>Name</th>
        <th>Password</th>
        <th>Actions</th>
    </tr>
    </thead>
<%secrets.forEach(item => {%>
    <tbody>
    <tr>
        <td><%= item.secret%></td>
        <td><a class="text-info" href="<%= item.url%>" target="_blank"><%= item.url%></a></td>
        <td><%= item.name%></td>
        <td><%= item.pwd%></td>
        <td><a class="text-info" href="edit?secret=<%= item.secret%>"><i class="fas fa-pencil-alt"></i></a>
        &nbsp;&nbsp;&nbsp;
        <a class="text-info" href="delete?secret=<%= item.secret%>"><i class="far fa-trash-alt"></i></a></td>
    </tr>
    </tbody>
<%});%>
</table>