<div>
<%if (secret != null) {%>
<form action="/modifySecret" method="post">
    <input type="hidden" name="_id" value="<%=secret._id%>">
    <label for="secret">Secret</label>
    <input type="text" name="secret" placeholder="Secret Name" value="<%=secret.secret%>">
    <label for="url">URL</label>
    <input type="url" name="url" placeholder="https://host:port" value="<%=secret.url%>">
    <label for="name">Name</label>
    <input type="text" name="name" placeholder="Name" value="<%=secret.name%>">
    <label for="pwd">Password</label>
    <input type="text" name="pwd" placeholder="Password" value="<%=secret.pwd%>">
    <input type="submit" value="Submit">
    <input type="button" value="Cancel" onclick="javascript:window.history.back()">
  </form>
<%= secret.secret%>, <%= secret.url%>, <%= secret.name%>, <%= secret.pwd%>
<%}else {%>
<form action="/saveSecret" method="post">
    <label for="secret">Secret</label>
    <input type="text" name="secret" placeholder="Secret Name">
    <label for="url">URL</label>
    <input type="url" name="url" placeholder="https://host:port">
    <label for="name">Name</label>
    <input type="text" name="name" placeholder="Name">
    <label for="pwd">Password</label>
    <input type="text" name="pwd" placeholder="Password">
    <input type="submit" value="Submit">
    <input type="button" value="Cancel" onclick="javascript:window.history.back()">
  </form>
  <%}%>
  </div>